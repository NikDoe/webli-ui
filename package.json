{
	"name": "webli-ui",
	"version": "0.3.2",
	"description": "library of UI components for react w/ typescript project",
	"keywords": [
		"ui-kit",
		"react",
		"typescript"
	],
	"contributors": [
		{
			"name": "Maxim Myatenko"
		},
		{
			"name": "Nikita Kuzmenko"
		}
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/NikDoe/webli-ui"
	},
	"type": "module",
	"main": "./dist/index.umd.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.umd.cjs"
		},
		"./dist/style.css": "./dist/style.css"
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"prepare": "husky install",
		"pretty": "prettier --write .",
		"pub:p": "npm run build && npm version patch && npm publish",
		"pub:min": "npm run build && npm version minor && npm publish",
		"pub:maj": "npm run build && npm version major && npm publish"
	},
	"devDependencies": {
		"@types/node": "^18.13.0",
		"@types/react": "^18.0.27",
		"@types/react-dom": "^18.0.10",
		"@types/uuid": "^9.0.1",
		"@typescript-eslint/eslint-plugin": "^5.50.0",
		"@typescript-eslint/parser": "^5.50.0",
		"@vitejs/plugin-react": "^3.1.0",
		"clsx": "^1.2.1",
		"eslint": "^8.33.0",
		"eslint-plugin-react": "^7.32.2",
		"husky": "^8.0.0",
		"lint-staged": "^13.1.0",
		"prettier": "2.8.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sass": "^1.58.0",
		"stylelint": "^14.16.1",
		"stylelint-config-prettier": "^9.0.4",
		"stylelint-config-recommended-scss": "^8.0.0",
		"stylelint-config-standard": "^29.0.0",
		"typescript": "^4.9.3",
		"uuid": "^9.0.0",
		"vite": "^4.1.0",
		"vite-plugin-dts": "^1.7.2",
		"vite-plugin-svgr": "^2.4.0"
	},
	"peerDependencies": {
		"react": "*",
		"react-dom": "*"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": "npx eslint --fix",
		"*.{js,jsx,ts,tsx,html,css|scss,json,cjs,md}": "npx prettier --write"
	},
	"files": [
		"dist/**/*"
	]
}
